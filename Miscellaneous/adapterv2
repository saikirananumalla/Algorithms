SELECT adapter.adapter_id, adapter.tenant_id as sourcetenant, data_selector.model_name as sourcemodel, adapter_destination.tenant_id as destinationtenant, destination_model 
FROM eagle_ci_publisher_svc.adapter join eagle_ci_publisher_svc.adapter_destination 
on adapter.adapter_id= adapter_destination.adapter_id join eagle_ci_publisher_svc.data_selector on adapter.data_selector_id=data_selector.data_selector_id;